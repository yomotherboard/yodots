         {
          :des "Yabai - Focus Mode"
          :rules [:tab-mode
                  [:c [:yabai-focus-space "Code"]]
                  [:d [:yabai-focus-space "Default"]]
                  [:1 [:yabai-focus-space "custom1"]]
                  [:2 [:yabai-focus-space "custom2"]]
                  [:3 [:yabai-focus-space "custom3"]]
                  [:4 [:yabai-focus-space "custom4"]]
                  [:5 [:yabai-focus-space "custom5"]]
                  [:6 [:yabai-focus-space "custom6"]]
                  [:7 [:yabai-focus-space "custom7"]]
                  [:8 [:yabai-focus-space "custom8"]]
                  [:9 [:yabai-focus-space "custom9"]]
                  [:h [:yabai-focus-window "west"]]
                  [:j [:yabai-focus-window "south"]]
                  [:k [:yabai-focus-window "north"]]
                  [:l [:yabai-focus-window "east"]]
                  ]
          }
          {
           :des "Yabai - Window Mode"
           :rules [
                   [:!Oh [:yabai-move "west"]]
                   [:!Oj [:yabai-move "south"]]
                   [:!Ok [:yabai-move "north"]]
                   [:!Ol [:yabai-move "east"]]
                   [:!Oc [[:yabai-space "Code"][:yabai-focus-space "Code"]]]
                   [:!Od [[:yabai-space "Default"][:yabai-focus-space "Default"]]]
                   [:!Os ["~/.script/yabai/new_split_view.sh"]]
                   [:!O1 [:yabai-new-space "1"]]
                   [:!O2 [:yabai-new-space "2"]]
                   [:!O3 [:yabai-new-space "3"]]
                   [:!O4 [:yabai-new-space "4"]]
                   [:!O5 [:yabai-new-space "5"]]
                   [:!O6 [:yabai-new-space "6"]]
                   [:!O7 [:yabai-new-space "7"]]
                   [:!O8 [:yabai-new-space "8"]]
                   [:!O9 [:yabai-new-space "9"]]
                   ]
           }
         {
          :des "Yabai - Insert Mode"
          :rules [:yabai-window-mode
                  ;[:c [:yabai-space "Code"] nil {:afterup [:yabai-focus-space "Code"]}]
                  [:h [[:yabai-insert "west"]] nil {:afterup [:!Cn]}]
                  [:j [[:yabai-insert "south"]] nil {:afterup [:!Cn]}]
                  [:k [[:yabai-insert "north"]] nil {:afterup [:!Cn]}]
                  [:l [[:yabai-insert "east"]] nil {:afterup [:!Cn]}]
                  ]
          }
         {
          :des "Yabai - Resize Mode"
          :rules [:yabai-resize-mode
                  [:h [:yabai-resize "left"]]
                  [:j [:yabai-resize "bottom"]]
                  [:k [:yabai-resize "top"]]
                  [:l [:yabai-resize "right"]]
                  ]
          }       
         {
          :des "Hyper Mode Test"
          :rules [:hyper-mode
                  [[:a :h] [:opena "Audio Hijack"]]
                  [[:a :l] [:opena "Alfred Preferences"]]
                  [:c [:opena "Chromium"]]
                  [:e [:opena "Mail"]]
                  [:f [:opena "Finder"]]
                  [:g [:opena "Google Chrome"]]
                  [:h [:open-chromium "http://hassio:8123/lovelace-yaml/default_view"]]
                  [[:k :e] [:opena "Karabiner-EventViewer"]]
                  [[:k :p] [:opena "Karabiner-Elements"]]
                  [[:k :m] [:opena "Keyboard Maestro"]]
                  [:l [:opena "TeXShop"]]
                  [:m [:opena "Messages"]]
                  [:p [:opena "OmniPlan"]]
                  [:r [:opena "Reminders"]]
                  [:s [:opena "Script Editor"]]
                  [:t [:opena "Terminal"]]
                  [:return_or_enter [["vim-mode" 1]["normal-mode" 1]] :!vim-mode]
                  [:return_or_enter [["vim-mode" 0]["normal-mode" 0]["insert-mode" 0]] :vim-mode]
                  ]
          }
         {
          :des "Config Launcher"
          :rules [:fn-mode
                  [:c [:vim "cfc"]]
                  [:k [:vim "cfk"]]
                  [:v [:vim "cfv"]]
                  [:z [:vim "cfz"]]
                  ]
          }
         {
          :des "Audio Mode"
          :rules [:audio-mode
                  [:1 [:set-volume "1"]]
                  [:2 [:set-volume "2"]]
                  [:3 [:set-volume "3"]]
                  [:4 [:set-volume "4"]]
                  [:5 [:set-volume "5"]]
                  [:6 [:set-volume "6"]]
                  [:7 [:set-volume "7"]]
                  [:8 [:set-volume "8"]]
                  [:9 [:set-volume "9"]]
                  [:0 [:set-volume "0"]]
                  [:hyphen [:bluetooth-p "0"]]
                  [:p [:bluetooth "Echo Plus-4BF"]]
                  [:s [[:trigger "ah.fix" "yomotherboard.config.audio" "Broken Speaker Workaround"]["ah-running" 1]]]
                  ]
          }
         {
          :des "Network Mode"
          :rules [:network-mode
                  [:y [:new-cl "ssh ymb"]]
                  [:h [:new-cl "ssh root@hassio"]]
                  [:l [:new-cl "ssh lbk-hass"]]
                  ]
          }
         {
          :des "Email Launcher"
          :rules [:email-launcher
                  [:p [:open-gmail "0"]]
                  [:b [:open-gmail "2"]]
                  ]
          }
