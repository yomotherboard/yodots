          {
           :des "Switch Numbers w/ Symbols"
           :rules [:!symbol-switch
                   [:f1 ["symbol-switch" 1]]
                   :symbol-switch
                   [:f1 ["symbol-switch" 0]]
                   [:1 :!S1]
                   [:2 :!S2]
                   [:3 :!S3]
                   [:4 :!S4]
                   [:5 :!S5]
                   [:6 :!S6]
                   [:7 :!S7]
                   [:8 :!S8]
                   [:9 :!S9]
                   [:0 :!S0]
                   [{:key :1 :modi [:shift]} :1]
                   [{:key :2 :modi [:shift]} :2]
                   [{:key :3 :modi [:shift]} :3]
                   [{:key :4 :modi [:shift]} :4]
                   [{:key :5 :modi [:shift]} :5]
                   [{:key :6 :modi [:shift]} :6]
                   [{:key :7 :modi [:shift]} :7]
                   [{:key :8 :modi [:shift]} :8]
                   [{:key :9 :modi [:shift]} :9]
                   [{:key :0 :modi [:shift]} :0]
                   ]  
           }
          {
           :des "cliclick test"
           :rules [:Chrome
                   [[:i :j :k] [:type ""]]
                ]
          }
